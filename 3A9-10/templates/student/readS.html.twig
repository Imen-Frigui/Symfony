<h1>La liste des étudiants </h1>
<table>
    <tr>
        <th>NSC</th>
        <th>EMAIL</th>
        <th>CLASSE</th>  
    </tr>
    {%for i in students %}
    <tr>
        <td>{{i.nsc}}</td>
        <td>{{i.email}}</td>
        <td>{{i.idClassroom.name}}</td>
    </tr>
    {%endfor%}
</table>
<h1>La liste des étudiants ordonée</h1>
<table>
<tr>
  <th>NSC</th>
  <th>Email</th>
  <th>Nom de la classe</th>
</tr>
<tr>
  {% for i in so %}
  <td>{{i.nsc}}</td>
  <td>{{i.email}}</td>
  <td>{{i.idClassroom.name}}</td>
</tr>
{% endfor %}
</table>